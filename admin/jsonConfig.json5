{
  "type": "tabs",
  "i18n": true,
  "tabsStyle": {
    "width": "calc(100% - 100px)"
  },
  "items": {
    "MainSettings": {
      "type": "panel",
      "label": "Main settings",
      "items": {
        "icontest": {
          "type": "staticImage",
          "src": "daswettercom.png",
          "newLine": true,
          "xs": 12,
          "sm": 2,
          "md": 2,
          "lg": 1,
          "xl": 1
        },
        "meteoredURL": {
          "type": "staticLink",
          "text": "Meteored dashboard",
          "href": "https://dashboard.meteored.com/de",
          "button": true,
          "icon": "info",
          "newLine": false,
          "xs": 12,
          "sm": 12,
          "md": 6,
          "lg": 4,
          "xl": 4
        },
        "dividerHdr1": {
          "newLine": true,
          "type": "header",
          "text": "general configuration",
          "size": 2
        },
        "ApiKey": {
          "newLine": true,
          "type": "text",
          "label": "api key",
          "help": "API key from DasWetter website",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "parseInterval": {
          "newLine": true,
          "type": "number",
          "label": "parse_interval",
          "help": "parse interval in [min]",
          "min": 40,
          "max": 600,
          "default": 120,
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "parseTimeout": {
          "newLine": false,
          "type": "number",
          "label": "parse_timeout",
          "help": "maximim parse timeout before no response error [sec]",
          "min": 10,
          "max": 60,
          "default": 10,
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },

        "dividerHdr2": {
          "newLine": true,
          "type": "header",
          "text": "location configuration",
          "size": 2
        },
        "locations": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "location settings",
          "showSecondAddAt": 5,
          "noDelete": false,
          "items": [
            {
              "type": "checkbox",
              "attr": "IsActive",
              "width": "5% ",
              "title": "IsActive",
              "tooltip": "enable location",
              "filter": false,
              "sort": false,
              "default": false
            },
            {
              "type": "text",
              "attr": "postcode",
              "width": "20% ",
              "title": "postcode",
              "tooltip": "postcode of location",
              "filter": false,
              "sort": false,
              "default": false
            },
            {
              "type": "text",
              "attr": "city",
              "width": "30% ",
              "title": "city",
              "tooltip": "city of location",
              "filter": false,
              "sort": false,
              "default": false
            },
            {
              "type": "checkbox",
              "attr": "useDailyForecast",
              "width": "5% ",
              "title": "useDailyForecast",
              "tooltip": "enable download of daily forecast",
              "filter": false,
              "sort": false,
              "default": true
            },
            {
              "type": "checkbox",
              "attr": "useHourlyForecast",
              "width": "5% ",
              "title": "useHourlyForecast",
              "tooltip": "enable download of hourly forecast",
              "filter": false,
              "sort": false,
              "default": true
            }
          ]
        }


      }
    },
    "Symbols": {
      "type": "panel",
      "label": "Icons",
      "items": {
        "iconSet": {
          "newLine": true,
          "type": "select",
          "label": "Icon set to select",
          "options": [
            {
              "label": { "en": "galeria1" },
              "value": 1
            },
            {
              "label": { "en": "galeria2" },
              "value": 2
            },
            {
              "label": { "en": "galeria3" },
              "value": 3
            },
            {
              "label": { "en": "galeria4" },
              "value": 4
            },
            {
              "label": { "en": "galeria5" },
              "value": 5
            },
            {
              "label": { "en": "galeria6" },
              "value": 6
            },
            {
              "label": { "en": "custom" },
              "value": 7
            }
          ],
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "UsePNGorOriginalSVG": {
          "newLine": true,
          "type": "checkbox",
          "label": "use_png_or_svg",
          "hidden": "if (data.iconSet != 5) return true;",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "UseColorOrBW": {
          "newLine": true,
          "type": "checkbox",
          "label": "use_color_or_bw",
          "hidden": "if (data.iconSet != 5) return true;",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "custom_path_hint": {
          "type": "staticText",
          "text": "custom_path_hint",
          "hidden": "if (data.iconSet != 7) return true;",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12
        },
        "CustomPath": {
          "newLine": true,
          "type": "text",
          "label": "custom_path",
          "hidden": "if (data.iconSet != 7) return true;",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "CustomPathExt": {
          "newLine": true,
          "type": "text",
          "label": "custom_path_ext",
          "hidden": "if (data.iconSet != 7) return true;",
          "default": ".png",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        }
      }
    },
    "WindIcons": {
      "type": "panel",
      "label": "WindIcons",
      "items": {
        "windiconSet": {
          "newLine": true,
          "type": "select",
          "label": "Icon set to select",
          "options": [
            {
              "label": { "en": "Beaufort-White" },
              "value": 1
            },
            {
              "label": { "en": "galeria2-Beaufort" },
              "value": 2
            },
            {
              "label": { "en": "galeria1" },
              "value": 3
            },
            {
              "label": { "en": "custom" },
              "value": 4
            }
          ],
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "windcustom_path_hint": {
          "type": "staticText",
          "text": "custom_path_hint",
          "hidden": "if (data.windiconSet != 4) return true;",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12
        },
        "WindCustomPath": {
          "newLine": true,
          "type": "text",
          "label": "custom_path",
          "hidden": "if (data.windiconSet != 4) return true;",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "WindCustomPathExt": {
          "newLine": true,
          "type": "text",
          "label": "custom_path_ext",
          "hidden": "if (data.windiconSet != 4) return true;",
          "default": ".png",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        }
      }
    },
    "MoonIcons": {
      "type": "panel",
      "label": "MoonIcons",
      "items": {
        "mooniconSet": {
          "newLine": true,
          "type": "select",
          "label": "Icon set to select",
          "options": [
            {
              "label": { "en": "original" },
              "value": 1
            },
            {
              "label": { "en": "custom" },
              "value": 2
            }
          ],
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "mooncustom_path_hint": {
          "type": "staticText",
          "text": "custom_path_hint",
          "hidden": "if (data.mooniconSet != 2) return true;",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12
        },
        "MoonCustomPath": {
          "newLine": true,
          "type": "text",
          "label": "custom_path",
          "hidden": "if (data.mooniconSet != 2) return true;",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        },
        "MoonCustomPathExt": {
          "newLine": true,
          "type": "text",
          "label": "custom_path_ext",
          "hidden": "if (data.mooniconSet != 2) return true;",
          "default": ".png",
          "xs": 12,
          "sm": 12,
          "md": 4,
          "lg": 4,
          "xl": 4
        }
      }
    }
  }
}